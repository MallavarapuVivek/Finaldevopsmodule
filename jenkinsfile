pipeline{
  agent any
  stages{
      stage('Deploy'){
              steps{
                    echo "deploy Successful"
                    bat "mvn compile"
                   }
            }
      stage('Build'){
              steps{
                    echo "Build Successful"
                    bat "mvn clean"
                   }
            }
      stage('Test'){
              steps{
                    echo "Test Successful"
                    bat "mvn test"
                   }
            }
    }
}
